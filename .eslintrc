{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    // "comma-dangle": ["error", {
    //   "arrays": "never",
    //   "objects": "never",
    //   "imports": "never",
    //   "exports": "never",
    //   "functions": "never"
    // }],
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"],
    "max-classes-per-file": "off",
    "max-len": ["error", { "code": 140 }]
  }
}